server {
    listen 80;
    server_name 44.243.39.112;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /food/FoodieTravels;
    }

    location / {
        include uwsgi_params;   # proxy_params;
        proxy_pass http://unix:/run/gunicorn.sock;
    }
}